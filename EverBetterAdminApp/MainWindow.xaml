<Window x:Class="EverBetterAdminApp.MainWindow"
                xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation" 
        xmlns:local="clr-namespace:EverBetterAdminApp"
        Title="EverBetter Administrator Portal" Height="371" Width="605">
    <Window.DataContext>
        <local:MainWindowViewModel />
    </Window.DataContext>
    <Grid Margin="0,0,-178,-37">
        <telerik:RadTabControl x:Name="TabControl" Margin="0,34,0,0">
            <telerik:RadTabItem Header="Change Survey Questions"/>
            <telerik:RadTabItem Header="Suspend/Ban User"/>
            <telerik:RadTabItem Header="Match Patients to Clinicians"/>
        </telerik:RadTabControl>

        <TextBlock x:Name="Welcometxt" Text="Welcome Placeholder" Margin="0,0,329,363" />
        <Button Content="Log out" Click="Logoutbtn_Click"  x:Name="Logoutbtn" Margin="567,0,0,363"></Button>
    </Grid>
</Window>
